{
    "name": "create-codeceptjs",
    "version": "0.1.7",
    "description": "Install CodeceptJS with all its dependencies in a single run",
    "main": "index.js",
    "bin": {
        "create-codeceptjs": "index.js"
    },
    "scripts": {
        "codeceptjs": "codeceptjs run --steps",
        "codeceptjs:headless": "HEADLESS=true codeceptjs run --steps",
        "codeceptjs:ui": "codecept-ui --app",
        "codeceptjs:demo": "codeceptjs run --steps -c node_modules/@codeceptjs/examples/codecept.puppeteer.conf.js",
        "codeceptjs:demo:headless": "HEADLESS=true codeceptjs run --steps -c node_modules/@codeceptjs/examples/codecept.puppeteer.conf.js",
        "codeceptjs:demo:ui": "codecept-ui --app  -c node_modules/@codeceptjs/examples/codecept.puppeteer.conf.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/codecept-js/create-codeceptjs.git"
    },
    "keywords": [
        "codeceptjs",
        "bdd",
        "installer",
        "e2e"
    ],
    "author": "Michael Bodnarchuk",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/codecept-js/create-codeceptjs/issues"
    },
    "homepage": "https://github.com/codecept-js/create-codeceptjs#readme",
    "dependencies": {
        "cfonts": "^2.8.5",
        "chalk": "^4.1.0",
        "commander": "^5.1.0",
        "cross-spawn": "^7.0.3",
        "fs-extra": "^9.0.1",
        "semver": "^7.3.2"
    },
    "devDependencies": {
        "@codeceptjs/configure": "^0.5.2",
        "@codeceptjs/examples": "^1.2.1",
        "@codeceptjs/ui": "^0.3.8",
        "codeceptjs": "^3.0.7",
        "playwright": "^1.2.1",
        "puppeteer": "^5.5.0"
    }
}
